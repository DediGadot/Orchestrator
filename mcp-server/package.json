    "ioredis": "^5.3.0",
    "child_process": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "mcp",
    "claude-code",
    "distributed",
    "agents",
    "orchestration"
