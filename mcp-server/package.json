{
  "name": "distributed-coding-orchestrator-mcp",
  "version": "1.0.0",
  "description": "MCP server for distributed coding agent orchestration",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "distributed-coding-orchestrator": "build/index.js"
  },
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
